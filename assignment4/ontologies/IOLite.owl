        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#Depiction"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wn noun: The arts of drawing or painting or printmaking</rdfs:comment>
        <rdfs:label xml:lang="en">Graphic art</rdfs:label>
        <rdfs:label xml:lang="it">Opera grafica</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#IconicObject -->

