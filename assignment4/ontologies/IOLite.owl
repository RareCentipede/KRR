        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An information object represented in an IconicLanguage</rdfs:comment>
        <rdfs:label xml:lang="en">Iconic object</rdfs:label>
        <rdfs:label xml:lang="it">Oggetto iconico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#KOS -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#KOS">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#DataStructure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge Organization Systems: thesauri, terminologies, classification schemes, subject hierarchies, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Knowledge organization system</rdfs:label>
        <rdfs:label xml:lang="it">Sistema di organizzazione della conoscenza</rdfs:label>
