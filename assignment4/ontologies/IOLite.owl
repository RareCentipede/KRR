        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The text of a contract</rdfs:comment>
        <rdfs:label xml:lang="en">Contract text</rdfs:label>
        <rdfs:label xml:lang="it">Testo di un contratto</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#DBSchema -->

